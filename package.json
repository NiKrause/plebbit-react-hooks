{
  "name": "@plebbit/plebbit-react-hooks",
  "engines": {
    "node": "14 || 16"
  },
  "keywords": [
    "plebbit",
    "subplebbit"
  ],
  "repository": "github:plebbit/plebbit-react-hooks",
  "license": "GPL-2.0",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start": "tsc --watch",
    "lint": "yarn run eslint --fix --ext .ts,.tsx .",
    "prettier": "prettier --write src",
    "test": "jest",
    "test:e2e": "jest --verbose -c test/e2e/setup/jest.config.js --runInBand",
    "webpackdev": "webpack serve --config test/e2e/setup/webpack.config.js",
    "ipfs": "DEBUG=plebbit-js:* node test/e2e/setup/ipfs"
  },
  "dependencies": {
    "@plebbit/plebbit-js": "https://github.com/plebbit/plebbit-js.git",
    "@types/debug": "4.1.7",
    "@types/uuid": "8.3.4",
    "assert": "2.0.0",
    "debug": "4.3.3",
    "localforage": "1.10.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@playwright/test": "1.12.1",
    "@testing-library/react-hooks": "8.0.0",
    "@tsconfig/recommended": "1.0.1",
    "@types/jest": "27.0.2",
    "@types/node": "16.9.0",
    "@types/react": "17.0.21",
    "@types/react-dom": "18.0.3",
    "@types/react-router-dom": "5.3.2",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "browser-crypto": "1.4.0",
    "buffer": "6.0.3",
    "eslint": "8.4.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "get-port": "5.1.1",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.2.4",
    "jest-playwright-preset": "1.7.0",
    "path-browserify": "1.0.1",
    "playwright-chromium": "1.14.1",
    "prettier": "2.3.2",
    "process": "0.11.10",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router-dom": "5.3.0",
    "react-test-renderer": "18.1.0",
    "ts-jest": "27.0.5",
    "ts-loader": "9.3.0",
    "typescript": "4.5.4",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.0",
    "window-or-global": "1.0.1"
  }
